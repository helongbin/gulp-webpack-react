{
  "env": {
    "browser": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 6, //指定ECMAScript支持的版本，6为ES6
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true//启动JSX
    },
    "rules": {
      "max-len": ["error", 120],
      "quotes": ["error", "double", "avoid-escape"]
    }
  }
}